#include <iostream>
using namespace std;

long sumsquares_list (long x[5], long size)
{
  long a=0, i;
  for (i=0; i<6; i++)
  {
    a= a + (x[i]*x[i]);
  }
    return a;
  }
  int main ()
  {
    long list[5];
    cout << "Enter 5 numbers" << endl;
    cout << "First= ";
    cin >> list[0];
    cout << "Second= ";
    cin >> list[1];
    cout << "Third= ";
    cin >> list[2];
    cout << "Fourth= ";
    cin >> list[3];
    cout << "Fifth= ";
    cin >> list[4];
    cout << "The sum of the squares of the numbers is= " << sumsquares_list(list,5);

    return 0;
  }
